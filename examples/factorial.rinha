let mult = fn (x, m) => {        
    let rec = fn (b, k) => {
        if(k > 1){                        
            rec(b + x, k - 1)
        }else{
            b
        }
    };
    rec(x, m)
};
let factorial = fn (n) => {    
    if (n == 1) {
        n
    }else{               
        mult(n, factorial(n - 1))        
    }
};

let res = factorial(5);
print(if (res == 120) {
	"OK: " + res
}else{
	"WRONG: " + res
})