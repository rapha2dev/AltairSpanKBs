let mult = fn (x, m) => {    
    let rec = fn (b, k) => {
        if(k > 1){                        
            rec(b + x, k - 1)
        }else{
            b
        }
    };
    rec(x, m)
};
let factorial = fn (n) => {    
    if (n == 1) {
        n
    }else{      
        let g = factorial(n - 1); 
        let k = n;           
        mult(k, g)        
    }
};

let res = factorial(5);
print(if (res == 120) {
	"   OK: " + res
}else{
	"   WRONG: " + res
})